{
  "name": "vue-use-openrouter",
  "version": "0.0.2",
  "description": "A Vue 3 composable for interacting with OpenRouter's AI models API",
  "repository": "ejfox/vue-use-openrouter",
  "author": "EJ Fox <ejfox@ejfox.com>",
  "license": "MIT",
  "keywords": [
    "vue",
    "composable",
    "openrouter",
    "ai",
    "chat",
    "gpt",
    "claude"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vite playground",
    "lint": "eslint --ext .ts,.js,.mjs,.cjs .",
    "lint:fix": "eslint --ext .ts,.js,.mjs,.cjs . --fix",
    "prepack": "unbuild",
    "release": "release-it",
    "test": "yarn lint && vitest run",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "predeploy": "npm run docs",
    "deploy": "gh-pages -d docs"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@nuxtjs/eslint-config-typescript": "latest",
    "@release-it/conventional-changelog": "^9.0.3",
    "@vitejs/plugin-vue": "^3.0.3",
    "c8": "latest",
    "eslint": "latest",
    "release-it": "^17.0.1",
    "standard-version": "latest",
    "typedoc": "^0.27.3",
    "typedoc-plugin-markdown": "^4.3.1",
    "typescript": "latest",
    "unbuild": "latest",
    "vite": "^3.0.7",
    "vitest": "latest",
    "vue": "^3.2.37"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=14"
  }
}
